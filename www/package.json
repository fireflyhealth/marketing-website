{
  "name": "firefly-health-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:static": "STATIC_BUILD=true next build && next-sitemap && esno ./scripts/copyMissingBContent",
    "start": "next start",
    "lint": "next lint",
    "test:all": "yarn test:types && yarn test:prettier && yarn test:lint && yarn test:queryByteLengths && yarn test:jest",
    "test:lint": "next lint",
    "test:queryByteLengths": "esno ./scripts/testQueryByteLengths.ts",
    "test:prettier": "prettier --check ./src",
    "test:types": "tsc --noEmit",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "update-mock-data": "NEXT_PUBLIC_SANITY_DATASET=production esno ./scripts/getMockData.ts",
    "update-site-data": "esno ./scripts/getSiteData.ts",
    "prepare": "yarn update-mock-data"
  },
  "dependencies": {
    "@axe-core/react": "^4.8.5",
    "@datadog/browser-rum": "^5.15.0",
    "@next/third-parties": "^14.1.4",
