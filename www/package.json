    "@sentry/nextjs": "^7.109.0",
    "@types/vimeo__player": "^2.18.3",
    "@vimeo/player": "^2.21.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "next": "14.0.4",
    "next-hubspot": "^1.2.0",
    "next-sitemap": "^4.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-swipeable": "^7.0.1",
    "what-input": "^5.2.12"
